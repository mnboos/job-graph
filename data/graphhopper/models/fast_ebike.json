{
  "distance_influence": 150,
  "priority": [
    {
      "if": "true",
      "multiply_by": "bike_priority"
    },
    {
      "if": "mtb_rating > 2",
      "multiply_by": "0"
    },
    {
      "if": "hike_rating > 1",
      "multiply_by": "0"
    },
    {
      "if": "!bike_access && (!backward_bike_access || roundabout)",
      "multiply_by": "0"
    },
    {
      "else_if": "!bike_access && backward_bike_access",
      "multiply_by": "0.2"
    },
    {
      "if": "max_slope >-15 && max_slope <= 15",
      "multiply_by": "2"
    },
    {
      "if": "road_class == STEPS",
      "multiply_by": "0"
    },
    {
      "if": "max_slope < -15",
      "multiply_by": "0.2"
    }
  ],
  "speed": [
    {
      "if": "true",
      "limit_to": "45"
    },
    {
      "if": "max_slope > 20",
      "multiply_by": "0.6"
    }
  ]
}